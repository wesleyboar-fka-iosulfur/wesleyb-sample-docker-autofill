{
  "opts": {
    "destination": "./docs/js",
    "recurse": true,
    "readme": "README.md",
    "template": "./node_modules/ink-docstrap/template",
    "tutorials": "./tutorials"
  },
  "source": {
    "include": "./src",
    "includePattern": ".+\\.js(doc)?$"
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
      "idInHeadings": true
  }
}