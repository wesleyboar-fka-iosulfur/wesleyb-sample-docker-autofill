{
  "name": "wesleyb-sample-schoology-auto-fill-web-service",
  "version": "1.0.0",
  "description": "A code sample of an auto-fill web service",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel docs test && npm run serve",
    "serve": "node --harmony --use_strict --optimize_for_size app.js",
    "docs": "jsdoc --configure .jsdocrc --recurse --readme README.md",
    "test": "echo \"Tests are not implemented\"",
    "watch": "npm-watch"
  },
  "author": "wesleyb@pm.me",
  "license": "UNLICENSED",
  "dependencies": {
    "koa": "^2.8.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-subdomain": "^3.0.0"
  },
  "devDependencies": {
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.3",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.6.0"
  },
  "watch": {
    "docs": {
      "patterns": "./",
      "ignore": [
        "./public",
        "./docs",
        "./data",
        "node_modules"
      ],
      "extensions": "md,js,jsdoc"
    }
  }
}
