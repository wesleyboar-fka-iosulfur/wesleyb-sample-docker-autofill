{
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "./node_modules/ink-docstrap/template",
    "tutorials": "./tutorials"
  },
  "source": {
    "include": "./",
    "exclude": ["./public", "./data", "./docs", "node_modules"],
    "includePattern": ".+\\.js(doc)?$"
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
      "idInHeadings": true
  }
}