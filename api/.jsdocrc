{
  "opts": {
    "destination": "./docs",
    "recurse": false,
    "readme": "README.md",
    "template": "./node_modules/ink-docstrap/template",
    "tutorials": "./tutorials"
  },
  "source": {
    "include": "./",
    "exclude": ["./docs", "node_modules"],
    "includePattern": ".+\\.js(doc)?$"
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
      "idInHeadings": true
  }
}